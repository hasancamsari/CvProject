@using CvProject.Models.Entity;
@model List<TBLCONTACT>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminDefault/_AdminMain.cshtml";
}
<h2>İletişim</h2>
<div >
    <table class="table table-bordered">
        <thead class="table-active">
            <tr>
                <th>TELEFON</th>
                <th>GITHUB</th>
                <th>GITHUB LİNKİ</th>
                <th>LINKEDIN</th>
                <th>LINKEDIN LİNK</th>
                <th>MAİL</th>
                <th>GÜNCELLE</th>
                <th>SİL</th>
                <th>DURUM</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.PHONE</td>
                    <td>@item.SMLGIT</td>
                    <td>
                        <a href="@item.LINKGIT" target="_blank">@item.LINKGIT</a>
                    </td>
                    <td>@item.SMLLINKED</td>
                    <td>
                        <a href="@item.LINKIN" target="_blank">@item.LINKIN</a>
                    </td>
                    <td>@item.LMAIL</td>
                    <td><a href="#" class="btn btn-warning btn-sm">GÜNCELLE</a></td>
                    <td><a href="#" class="btn btn-danger btn-sm">SİL</a></td>
                    <td class="text-center">
                        <input type="checkbox" style="width:20px; height:20px; cursor:pointer;" />
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<a class="btn btn-primary" href="@Url.Action("AddContact","AdminContact")">Yeni İletişim Ekle</a>

